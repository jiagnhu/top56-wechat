  display: block;
  text-align: center;
}

.order_inif_send view {
  line-height: 40rpx;
}

.order_inif_conle {
  height: 94rpx;
  width: 150rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-left: 20rpx;
  display: block;
  text-align: center;
}

.order_inif_conle view {
  line-height: 40rpx;
}

.order_inif_adderss {
  color: #000;
}

.order_inif_name {
  font-size: 24rpx;
}

.order_time {
  padding: 0rpx 24rpx;
  line-height: 64rpx;
  font-size: 24rpx;
}

.order_imag{
  margin: 20rpx 10rpx 0rpx 0rpx;
